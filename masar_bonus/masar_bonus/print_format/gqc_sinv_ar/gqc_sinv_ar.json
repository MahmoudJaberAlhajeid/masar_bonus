 "font_size": 14,
 "html": "<style>\r\n    @page {\r\n        size: A4;\r\n        margin: 20mm;\r\n    }\r\n\r\n    body {\r\n        font-family: Arial, sans-serif;\r\n        font-size: 14px;\r\n        margin: 0;\r\n        padding: 0;\r\n    }\r\n\r\n    .container {\r\n        width: 100%;\r\n        padding: 0;\r\n        margin: 0 auto;\r\n    }\r\n\r\n    .header, .footer {\r\n        text-align: center;\r\n        margin-bottom: 20px;\r\n    }\r\n\r\n    .header img {\r\n        width: 50px;\r\n        height: auto;\r\n        display: block;\r\n        margin: 0 auto;\r\n    }\r\n\r\n    .header-right {\r\n        text-align: right;\r\n        margin-top: 20px;\r\n    }\r\n\r\n    .header-right strong {\r\n        font-size: 18px;\r\n    }\r\n\r\n    .invoice-number {\r\n        font-size: 14px;\r\n        margin-top: 5px;\r\n    }\r\n\r\n    hr {\r\n        margin: 20px 0;\r\n    }\r\n\r\n    .details-section {\r\n        display: flex;\r\n        justify-content: flex-end;\r\n        gap: 20px;\r\n        margin-left: 20px;\r\n    }\r\n\r\n    .details-left {\r\n        width: 45%;\r\n        margin-left: 20px;\r\n    }\r\n\r\n    .details-right {\r\n        width: 45%;\r\n        text-align: right;\r\n    }\r\n\r\n    .details-right .details-info, \r\n    .details-left .details-info {\r\n        display: flex;\r\n        justify-content: flex-start;\r\n        align-items: center;\r\n        margin-bottom: 5px;\r\n    }\r\n\r\n    .details-right .details-info strong,\r\n    .details-left .details-info strong {\r\n        min-width: 150px;\r\n        display: inline-block;\r\n    }\r\n\r\n    .details-right .details-info span, \r\n    .details-left .details-info span {\r\n        margin-left: 5px;\r\n        text-align: left;\r\n    }\r\n\r\n    table {\r\n        width: 100%;\r\n        border-collapse: collapse;\r\n        margin-top: 20px;\r\n    }\r\n\r\n    table th, table td {\r\n        border: 1px solid #000;\r\n        padding: 8px;\r\n        text-align: center;\r\n    }\r\n\r\n    thead {\r\n        background-color: #f0f0f0;\r\n    }\r\n\r\n    .total-section {\r\n        display: flex;\r\n        justify-content: flex-start;\r\n        gap: 50px;\r\n        margin-top: 20px;\r\n    }\r\n\r\n    .total-section-right {\r\n        width: 50%;\r\n    }\r\n\r\n    .total-section-left {\r\n        width: 50%;\r\n        text-align: left;\r\n    }\r\n\r\n    .total-section-left strong {\r\n        display: inline-block;\r\n        width: 200px;\r\n    }\r\n\r\n    .no-border-table {\r\n        font-size: 12px;\r\n        border: none;\r\n    }\r\n    .small-font-table {\r\n    font-size: 12px; \r\n    }\r\n\r\n    .no-border-table td {\r\n        border: none; \r\n        padding: 5px;\r\n    }\r\n\r\n    .footer {\r\n        position: absolute;\r\n        bottom: 0;\r\n        width: 100%;\r\n        text-align: center;\r\n        margin-top: 40px;\r\n    }\r\n\r\n    .footer img {\r\n        width: 50px;\r\n        height: auto;\r\n    }\r\n</style>\r\n\r\n<div class=\"container\">\r\n    <div class=\"header\">\r\n        <img src=\"/assets/yourapp/images/logo.png\" alt=\"\u0627\u0644\u0634\u0639\u0627\u0631\" />\r\n    </div>\r\n\r\n    <div class=\"header-right\">\r\n        <strong>SALES INVOICE</strong><br>\r\n        <div class=\"invoice-number\">{{doc.name}}</div>\r\n    </div>\r\n\r\n    <hr>\r\n\r\n    <table class=\"no-border-table\">\r\n        <tbody>\r\n            <tr>\r\n                <td>\u0627\u0633\u0645 \u0627\u0644\u0639\u0645\u064a\u0644: {{doc.customer}}</td>\r\n                <td>\u0627\u0644\u062a\u0627\u0631\u064a\u062e:{{doc.posting_date}}</td>\r\n                <td>\u0627\u0644\u0631\u0642\u0645 \u0627\u0644\u0636\u0631\u064a\u0628\u064a \u0644\u0644\u0634\u0631\u0643\u0629: {{doc.company_tax_id}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td>\u0627\u0644\u062d\u0627\u0644\u0629: {{doc.status}}</td>\r\n                <td>\u062a\u0627\u0631\u064a\u062e \u0627\u0633\u062a\u062d\u0642\u0627\u0642 \u0627\u0644\u0633\u062f\u0627\u062f: {{doc.due_date}}</td>\r\n                <td>\u062a\u0627\u0631\u064a\u062e \u0637\u0644\u0628 \u0627\u0644\u0634\u0631\u0627\u0621 \u0645\u0646 \u0627\u0644\u0639\u0645\u064a\u0644:{{doc.po_date}}</td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n\r\n<table class=\"small-font-table\">\r\n    <thead>\r\n        <tr style=\"background-color: #f0f0f0;\">\r\n            <th>\u0631.\u062a</th>\r\n            <th>\u0627\u0633\u0645 \u0627\u0644\u0633\u0644\u0639\u0629</th>\r\n            <th>\u0643\u0645\u064a\u0629</th>\r\n            <th>\u0639\u0644\u0627\u0648\u0629</th>\r\n            <th>\u0633\u0639\u0631 \u0627\u0644\u0633\u0644\u0639\u0629</th>\r\n            <th>\u0627\u0644\u0645\u062c\u0645\u0648\u0639</th>\r\n        </tr>\r\n    </thead>\r\n    <tbody>\r\n    {% for item in doc.items %}\r\n    <tr>\r\n        <td>{{ item.item_code }}</td>\r\n        <td>{{ item.item_name }}</td>\r\n        <td>{{ item.qty }}</td>\r\n        <td>{{ item.custom_bonus_qty or ''}}</td>\r\n        <td>{{ item.rate }} \u062f.\u0623</td>\r\n        <td>{{ item.amount }} \u062f.\u0623</td>\r\n    </tr>\r\n    {% endfor %}\r\n</tbody>\r\n</table>\r\n\r\n    <div class=\"total-section\">\r\n        <div class=\"total-section-right\">\r\n            {% set total_qty = 0 %}  \r\n            {% for item in doc.items %}\r\n                {% set total_qty = total_qty + item.qty %}\r\n            {% endfor %}\r\n            <strong>\u0627\u0644\u0643\u0645\u064a\u0629 \u0627\u0644\u0625\u062c\u0645\u0627\u0644\u064a\u0629:</strong> {{ doc.total_qty }}<br>\r\n            <strong>\u0627\u0644\u0625\u062c\u0645\u0627\u0644\u064a \u063a\u064a\u0631 \u0634\u0627\u0645\u0644 \u0627\u0644\u0636\u0631\u064a\u0628\u0629:</strong> <span> {{ doc.total }} \u062f.\u0623</span><br>\r\n            <strong>\u0645\u062c\u0645\u0648\u0639 \u0627\u0644\u0636\u0631\u0627\u0626\u0628 \u0648\u0627\u0644\u0631\u0633\u0648\u0645:</strong> <span>{{ doc.total_taxes_and_charges }} \u062f.\u0623</span><br>\r\n            <strong>\u0627\u0644\u0645\u062c\u0645\u0648\u0639 \u0627\u0644\u0625\u062c\u0645\u0627\u0644\u064a:</strong> <span>{{ doc.grand_total }} \u062f.\u0623</span><br>\r\n            \r\n            <strong>\u0628\u0627\u0644\u0643\u0644\u0645\u0627\u062a:</strong> \r\n             <span>{{ frappe.utils.in_words(doc.grand_total) }} \u062f\u064a\u0646\u0627\u0631 \u0623\u0631\u062f\u0646\u064a</span><br>\r\n\r\n             {% for amount in doc.payment_schedule %}\r\n            <strong>\u0627\u0644\u0645\u0628\u0644\u063a \u0627\u0644\u0645\u062f\u0641\u0648\u0639:</strong><span>{{ amount.payment_amount or ''}}</span><br>\r\n             {% endfor %}\r\n             \r\n            <strong>\u0627\u0644\u0627\u062a\u0635\u0627\u0644:</strong> <span>{{ doc.contact_display }}</span><br>\r\n            <strong>\u0631\u0642\u0645 \u0627\u0644\u062c\u0648\u0627\u0644:</strong> \r\n            <span>\r\n                {% if doc.contact_person %}\r\n                    {% set contact = frappe.get_doc('Contact', doc.contact_person) %}\r\n                    {{ contact.mobile_no }}\r\n                {% else %}\r\n                    \u063a\u064a\u0631 \u0645\u062a\u0648\u0641\u0631\r\n                {% endif %}\r\n            </span>\r\n            <br>\r\n        </div>\r\n    </div>\r\n\r\n    <div class=\"footer\">\r\n        <img src=\"/assets/yourapp/images/logo.png\" alt=\"\u0627\u0644\u0634\u0639\u0627\u0631\" />\r\n    </div>\r\n</div>\r\n",
